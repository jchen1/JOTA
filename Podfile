target 'OTPAutoFill' do
  platform :ios, '10.0'

  # Comment the next line if you don't want to use dynamic frameworks
  use_frameworks!

  # Pods for OTPAutoFill
  pod 'KeychainAccess'
  pod 'SwiftOTP'
end

target 'JOTA' do
  platform :ios, '10.0'

  # Comment the next line if you don't want to use dynamic frameworks
  use_frameworks!

  # Pods for JOTA
  pod 'KeychainAccess'
  pod 'SwiftOTP'
  pod 'Toaster'

  target 'JOTATests' do
    inherit! :search_paths
    # Pods for testing
  end

  target 'JOTAUITests' do
    inherit! :search_paths
    # Pods for testing
  end

end

