{
    "manifest_version": 2,
    "default_locale": "en",

    "name": "JOTA",
    "description": "Autofill MFA codes",
    "version": "1.0",

    "icons": {
        "48": "images/icon-48.png",
        "96": "images/icon-96.png",
        "128": "images/icon-128.png",
        "256": "images/icon-256.png",
        "512": "images/icon-512.png"
    },

    "background": {
        "scripts": [ "background.js" ],
        "persistent": false
    },

    "content_scripts": [{
        "js": [ "content.js" ],
        "matches": [ "https://*/*" ]
    }],

    "permissions": [ ]
}
